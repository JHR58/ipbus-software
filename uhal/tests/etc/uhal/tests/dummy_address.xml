<?xml version="1.0" encoding="ISO-8859-1"?>

<node id="SYSTEM">
    <node id="REG" address="0x0001" permission="rw"/>
    
    <!-- two registers with the same underlying address -->
    <node id="REG_READ_ONLY" address="0x0002" permission="r"/>
    <node id="REG_WRITE_ONLY" address="0x0002" permission="w"/>
    
    <node id="REG_WITHOUT_MASK" address="0x0003" permission="rw"/>
    <node id="REG_WITH_MASK" address="0x0003" mask="0x00ffff00" permission="rw"/>
    
    <node id="FIFO" address="0x0100" permission="rw" mode="non-incremental"/>

    <node id="MEM" address="0x0200" permission="rw" mode="incremental"/>

    <node id="SUBSYSTEM" module="file://etc/uhal/tests/dummy_level2_address.xml" />

</node>
